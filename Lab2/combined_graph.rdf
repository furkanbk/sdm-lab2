    <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10.5220/0006660004470451</ex:doi>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Paper_011961296d458fbf1d605e58f49497e7a8f6f011">
