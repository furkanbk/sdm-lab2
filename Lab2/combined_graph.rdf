    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">eye,gaze,use,humans,information,intentions,actions,sclera,interpretation,extends,show,also,control,user,human,beings,communicate,abbreviated,ways,dependent</ex:keywords>
    <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10.1155/2008/623725</ex:doi>
    <ex:citation_year rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2019</ex:citation_year>
