    <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></ex:doi>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_145009388"/>
