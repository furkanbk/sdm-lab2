    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">research,tools,users,data,causality,explanation,applications,database,surge,availability,information,great,demand,assist,understanding,today,exploration,rely,mostly,visualization</ex:keywords>
