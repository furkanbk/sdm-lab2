    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">database,animations,disciplines,concepts,many,computer,fundamental,courses,well,evaluation,topics,technology,affects,beyond,science,business,paper,describes,two,developed</ex:keywords>
