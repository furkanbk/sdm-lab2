    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">framework,games,observation,equivalence,consider,general,hybrid,systems,dynamics,system,players,partial,show,suffix,encoding,abstraction,decidability,computability,results,paper</ex:keywords>
