    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">detection,databases,research,emotion,past,decades,interaction,become,important,daily,lives,developed,many,directions,memory,depression,behavioural,deficiency,lie,hidden</ex:keywords>
