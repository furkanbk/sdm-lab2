    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">vtr,speech,database,vocal,tract,resonances,processing,automatic,techniques,frequency,phonetic,values,cv,vc,transitions,tracking,vtrs,formants,defined,known</ex:keywords>
