    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Forecasting Stock Market Prices Using Machine Learning and Deep Learning Models: A Systematic Review, Performance Analysis and Discussion of Implications-3</ex:title>
    <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10.3390/ijfs11030094</ex:doi>
    <ex:citation_year rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2020</ex:citation_year>
