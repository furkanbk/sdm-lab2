    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">database,corpus,paper,recorded,based,synthesis,emotions,creation,describes,emotional,speech,standard,basque,designed,twofold,purpose,used,also,allowing,study</ex:keywords>
