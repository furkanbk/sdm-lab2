    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Resisting Collusion by Game in Culture Web-5</ex:title>
    <ex:citation_year rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2022</ex:citation_year>
    <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10.1109/WETICE.2011.45</ex:doi>
