    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fundamental Tradeoffs and Constrained Coalitional Games in Autonomic Wireless Networks-1</ex:title>
    <ex:citation_year rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2021</ex:citation_year>
