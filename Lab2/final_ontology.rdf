        <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">uru,worlds,pearce,game,online,players,emergent,virtual,book,mmo,prologue,group,behavior,second,one,world,live,affordances,communities,play</ex:keywords>
