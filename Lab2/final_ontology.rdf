    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">game,system,ar,immersive,vr,developed,environments,reality,users,world,players,evaluation,paper,present,new,multiplayer,two,different,namely,virtual</ex:keywords>
