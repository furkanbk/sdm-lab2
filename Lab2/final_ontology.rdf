    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Is This a Game or a Learning Moment-7</ex:title>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_145364912"/>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_32041960"/>
