        <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bounds on the Price of stability of Undirected Network Design Games with Three Players</ex:title>
        <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10.1142/S0219265911002824</ex:doi>
        <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_1785624"/>
