    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bounds on the Price of stability of Undirected Network Design Games with Three Players-5</ex:title>
    <ex:year rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">2011</ex:year>
    <ex:keywords rdf:datatype="http://www.w3.org/2001/XMLSchema#string">games,upper,case,players,lower,bounds,bound,price,stability,number,multicast,broadcast,new,improved,ref,almost,seven,years,undirected,network</ex:keywords>
