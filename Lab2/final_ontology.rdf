    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Non-Cooperative Multicast and Facility Location Games-8</ex:title>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_1402943721"/>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_2495534"/>
