    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Cooperative Game Framework for QoS Guided Job Allocation Schemes in Grids-1</ex:title>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_1705486"/>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_9392149"/>
