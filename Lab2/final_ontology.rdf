    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Communitywide Database Designs for Tracking Innovation Impact: Comets, Stars and Nanobank-7</ex:title>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_30956207"/>
    <ex:written_by rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Author_2058161182"/>
