    <ex:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Early Detection of Aphid Infestation and Insect-Plant Interaction Assessment in Wheat Using a Low-Cost Electronic Nose (E-Nose), Near-Infrared Spectroscopy and Machine Learning Modeling-2</ex:title>
    <ex:cites_to rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Paper_00cb8781b1c991659b9ba6e525eaadf13fe8f359"/>
