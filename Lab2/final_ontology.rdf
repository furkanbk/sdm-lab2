    <ex:doi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10.1142/S0219265911002824</ex:doi>
    <ex:cites_to rdf:resource="http://www.semanticweb.org/kocak/ontologies/2024/4/AcademicPapers#Paper_003d8ba6b56262942c5ed88052fbd1282495966b"/>
